        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StaMonitor"
        mc:Ignorable="d"
        Title="MainWindow" Height="64.304" Width="271.456" Activated="Window_Activated">
    <Grid>
        <TextBox x:Name="EndTime" HorizontalAlignment="Left" Height="22" Margin="66,10,0,0" TextWrapping="Wrap" Text="1.0" VerticalAlignment="Top" Width="96" TextChanged="EndTime_TextChanged"/>
